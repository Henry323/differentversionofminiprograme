<!--logs.wxml-->
<view class="container"> 
 
 <view class="login-icon"> 
 <image class="login-img" src="../img/1.png"></image> 
 </view> 
 <view class="login-from"> 
 
 <!--账号-->
 <view class="inputView"> 
  <image class="nameImage" src="../img/23.png"></image> 
  <label class="loginLab">账号</label> 
  <input class="inputText" placeholder="account number" bindinput="usernameInput" /> 
 </view> 
 <view class="line"></view> 
 
 <!--密码-->
 <view class="inputView"> 
  <image class="keyImage" src="../img/keys.png"></image> 
  <label class="loginLab">密码</label> 
  <input class="inputText" password="true" placeholder="password" bindinput="passwordInput" /> 
 </view> 
 
 <!--按钮-->
 <view class="loginBtnView"> 
  <button class="loginBtn" type="primary" size="{{primarySize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="login">LOG IN</button> 
 </view> 

 <navigator url="forgetpassword">
   <view class="test">forgetpassword 
   </view>
</navigator>

 <navigator url="register">
   <view class="test">register</view></navigator>
 </view> 
</view>

